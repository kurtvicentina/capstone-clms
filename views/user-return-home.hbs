<div class="container pt-5">
<div class="row">
  <div class="col-6"><h1>Return List</h1></div>
 
</div>

<table class="table table-info table-striped table-hover text-center table-bordered border-info">
  <thead>
    <tr>
      <th scope="col">ISBN</th>
      <th scope="col">Title</th>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Contact Number</th>
      <th scope="col">Borrowed Date</th>
      <th scope="col">Returned Date</th>
    </tr>
</thead>
  <tbody>

    {{#each data}}
    <tr>
      <td>{{this.isbn}}</td>
      <td>{{this.title}}</td>
      <td>{{this.first_name}}</td>
      <td>{{this.last_name}}</td>
      <td>{{this.contact}}</td>
      <td>{{formatDate this.borrow_date}}</td>
      <td>{{formatDate this.actual_return_date}}</td>
    </tr>
    {{/each}}

  </tbody>
</table>
</div>